{
  "expo": {
    "name": "Iweft",
    "slug": "iweft",
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "iweft",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyD_x_jkpt33ZX96mrYSO4w65oRtLr9nxlo",
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.paras23.iweft",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID and Touch ID to verify the user's identity and protect sensitive data",
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSCalendarsUsageDescription": "This app needs access to your calendar to schedule events and reminders.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "iweft",
              "com.googleusercontent.apps.237084984780-oj2g0uh2v47fsh0a20fp01le3c8kslek"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAHmtNmFT8nsIb519PlVAEN-Zgtm02ig1s"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "meta-data": [
        {
          "name": "com.google.android.gms.wallet.api.enabled",
          "value": "true"
        }
      ],
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "You location is used to show your position on the map, get directions, estimate travel times for delivery purposes.",
          "locationWhenInUsePermission": "You location is used to show your position on the map, get directions, estimate travel times for delivery purposes.",
          "locationAlwaysPermission": "You location is used to show your position on the map, get directions, estimate travel times for delivery purposes."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photos to upload photos from your library.",
          "microphonePermission": "The microphone is used to record audio for use within the app.",
          "cameraPermission": "Camera usage to take pictures for your profile. Please allow camera access to continue."
        }
      ],
      ["expo-apple-authentication"],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "1518615575408915",
          "clientToken": "fde73919bc269ef295f20308f3725012",
          "displayName": "iWeft",
          "scheme": "fb1518615575408915",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.iweft",
          "enableGooglePay": true
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b19817b7-4bf6-4317-94d7-f524d68e4e25"
      }
    },
    "owner": "paras23"
  }
}
